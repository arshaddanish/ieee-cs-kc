<%- include ("../partials/head"); %>
<%- include ("../partials/admin-navbar-shadow"); %>
<%- include ("../partials/loader"); %>

<div id="page" class="site ">
  <div class="main-contain updates-padding loadah">
    <div class="container">
      <div id="content" class="site-content">
        <div class="rs-heading style3  vc_custom_1543920863426  ">
          <div class="title-inner">
            <!-- <span style="color: #28406d" class="sub-text">[News]</span> -->
            <h2 class="title" style="margin-bottom: 0;">Events</h2>
          </div>
          <!-- <div class="description">
            <p>Accelerating innovative young minds.</p>
          </div> -->
        </div>
        <div class="row padding-">
          <div class="col-md-12 ">
            <article id="post-2176" class="post-2176 page type-page status-publish hentry">
              <div class="entry-content">
                <div class="vc_row wpb_row vc_row-fluid vc_custom_1543998372065">
                  <div class="wpb_column vc_column_container vc_col-sm-12">
                    <div class="vc_column-inner">
                      <div class="wpb_wrapper">
                        <div class="rs-blog-grid rs-blog ">
                          <div class="row">
                            <% data.events.forEach(function(event) { %>

                            <div class="blog-item col-lg-4 col-md-12 style1 event-item">
                              <div class="event-card">
                                <div class="blog-img image-scale"> <img width="1200" height="800" src="<%= event.image %>" class="attachment-full size-full wp-post-image" class="attachment-post-thumbnail size-post-thumbnail wp-post-image event-img" alt="<%= event.name %>" srcset="<%= event.image %>" sizes="(max-width: 1200px) 100vw, 1200px" />
                                  <div class="blog-date"> <span class="date"><%= event.day %> </span> <span class="month"><%= event.month %>
                                    </span></div>
                                </div>
                                <div class="bottom-shadow1 ">
                                  <div class="blog-meta">
                                    <h3 class="blog-title"><%= event.title %></h3>
                                    <!-- <div class="blog-date"> <span class="author">By admin</span><span class="seperator">/</span> <span class="categories"><a href="../category/corporate/index.html">Corporate </a></span></div> -->
                                  </div>
                                  <div class="admin-btns">
                                    <!-- <div><a href="/admin/edit-event" class="btn btn-primary edit-btn">Edit</a></div> -->
                                    <div><a href="/admin/delete/events/<%= event.id %>" class="btn btn-danger delete-btn"  onclick="return confirm('Are you sure you want to delete this item?')">Delete</a></div>
                                  </div>
                                </div>
                              </div>
                            </div>

                            <% }) %>
                          </div>
                          <div class="pagination-area">
                            <div class="nav-links">
                              <% if(data.page-2> 1){ %>
                              <a class="next page-numbers" href="/admin/events?page=<%= data.page-1 %>">&laquo;</a>
                              <% } %>

                              <% for (var i=data.page-2 ; i < data.page; i++) { if (i> 0) { %>
                              <a class="next page-numbers" href="/admin/events?page=<%= i %>">
                                <%= i %>
                              </a>
                              <% }} %>

                              <span aria-current='page' class='page-numbers current'>
                                <%= data.page %>
                              </span>

                              <% for (var i=data.page+1 ; i < data.page+3; i++) { if (i <=data.no_of_pages)
						    { %>
                              <a class="next page-numbers" href="/admin/events?page=<%= i %>">
                                <%= i %>
                              </a>
                              <% }} %>

                              <% if(data.page+2 < data.no_of_pages){ %>
                              <a class="next page-numbers" href="/admin/events?page=<%= data.page+1 %>">&raquo;</a>
                              <% } %>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </article>
          </div>
        </div>
      </div>
    </div>
  </div>

  <script>
    document.getElementById("events19").classList.add("current-menu-ancestor");
    document.getElementById("home19").classList.remove("current_page_item", "current-menu-ancestor");
  </script>

  <%- include ("../partials/admin-footer"); %>