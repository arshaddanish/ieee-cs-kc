<%- include ("partials/head"); %>
<%- include ("partials/navbar-shadow"); %>

<% let year = new Date(data.date).getFullYear() %> 
<div id="page" class="site ">
  <div class="main-contain updates-padding">
    <div class="container">
      <div id="content">
        <div class="rs-heading style3  vc_custom_1543920863426  ">
          <div class="title-inner">
            <!-- <span style="color: #28406d" class="sub-text">[News]</span> -->
            <h2 class="title" style="margin-bottom: 0;">Updates /<span style="color: gray ; font-size: 1.5rem">
                <%= data.name %>
              </span></h2>
          </div>
        </div>
        <div class="rs-blog-details pt-70 pb-70">
          <div class="row padding-">
            <div class="col-md-12 ">
              <article id="post-2164" class="post-2164 post type-post status-publish format-standard has-post-thumbnail hentry category-corporate tag-business tag-corporate">
                <div class="bs-img"> <img src="<%= data.image %>" class="attachment-post-thumbnail size-post-thumbnail wp-post-image" alt="" srcset="<%= data.image %>" sizes="(max-width: 1200px) 100vw, 1200px" />
                    
                </div>
                <div class="single-content-full">
                  <div class="bs-desc update-content">
                    <h2 class="post-title">
                      <%= data.name %>
                    </h2>
                    <p>[<%= data.day %> <%= data.month %> <%= year %>]</p>
                    <p>
                      <%= data.description %>
                    </p>
                  </div>
                </div>
                <div class="clear-fix"></div>
              </article>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <script>
    document.getElementById("updates19").classList.add("current-menu-ancestor");
    document.getElementById("updates1923").classList.add("current-menu-ancestor");
    document.getElementById("home19").classList.remove("current_page_item", "current-menu-ancestor");
  </script>

  <%- include ("partials/footer"); %>